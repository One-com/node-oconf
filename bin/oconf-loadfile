#!/usr/bin/env node
// ex: filetype=javascript

var loader = require('../lib/loader'),
    optimist = require('optimist');

var argv = optimist
            .demand(1)
            .usage('Usage: $0 <filename.cjson>')
            .argv;

try {
    console.dir(loader(argv._[0]));
} catch (error) {
    console.error("Error:", error.message);
}
